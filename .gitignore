# Ignore directories generated by Composer
/vendor/
/web/core/
/web/libraries/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/

# Ignore environment information
/web/sites/*/settings.local.php
/web/sites/*/lando-settings.local.php

# Ignore Drupal's file directory
/web/sites/*/files/

# Ignore files generated by PhpStorm
/.idea/
.DS_Store

# Ignore node modules
node_modules

# Log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore mysql dumps but keep folder
/.dev/mysql_dumps/*
!/.dev/mysql_dumps/.gitkeep

# local env files
.env.local
.env.*.local

# Ignore built assets
/web/themes/custom/endanet/assets

# Development files
/.vagrant/
/.dev/config.yml
Vagrantfile
.lando.yml
.docksal

# DB dumps
*.sql

# Ignore Drupal's themes
/web/themes/custom/*/assets/*
/web/themes/custom/*/assets/css/*
/web/themes/custom/*/assets/js/*
/web/themes/custom/*/assets/img/*
