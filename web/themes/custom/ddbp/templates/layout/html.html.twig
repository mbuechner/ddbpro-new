{% set body_classes = [
  logged_in ? 'user-logged-in',
  not root_path ? 'path-frontpage' : 'path-' ~ root_path|clean_class,
  node_type ? 'page-node-type-' ~ node_type|clean_class,
  db_offline ? 'db-offline',
] %}

<!DOCTYPE html>
<html{{ html_attributes }}>
  <head>
    <head-placeholder token="{{ placeholder_token }}">
    <title>{{ head_title|safe_join(' | ') }}</title>
    {# Can not preload these fonts bc font versioning is currently enabled. #}
    {#
    <link rel="preload" href="/{{ directory }}/assets/fonts/inter-v11-latin-regular.woff2" as="font" crossorigin>
    <link rel="preload" href="/{{ directory }}/assets/fonts/inter-v11-latin-700.woff2" as="font" crossorigin>
    #}
    <css-placeholder token="{{ placeholder_token }}">
    <js-placeholder token="{{ placeholder_token }}">
    <link rel="apple-touch-icon" sizes="57x57" href="/{{ directory }}/src/images/favicon/apple-icon-57x57.png">
    <link rel="apple-touch-icon" sizes="60x60" href="/{{ directory }}/src/images/favicon/apple-icon-60x60.png">
    <link rel="apple-touch-icon" sizes="72x72" href="/{{ directory }}/src/images/favicon/apple-icon-72x72.png">
    <link rel="apple-touch-icon" sizes="76x76" href="/{{ directory }}/src/images/favicon/apple-icon-76x76.png">
    <link rel="apple-touch-icon" sizes="114x114" href="/{{ directory }}/src/images/favicon/apple-icon-114x114.png">
    <link rel="apple-touch-icon" sizes="120x120" href="/{{ directory }}/src/images/favicon/apple-icon-120x120.png">
    <link rel="apple-touch-icon" sizes="144x144" href="/{{ directory }}/src/images/favicon/apple-icon-144x144.png">
    <link rel="apple-touch-icon" sizes="152x152" href="/{{ directory }}/src/images/favicon/apple-icon-152x152.png">
    <link rel="apple-touch-icon" sizes="180x180" href="/{{ directory }}/src/images/favicon/apple-icon-180x180.png">
    <link rel="icon" type="image/png" sizes="192x192"  href="/{{ directory }}/src/images/favicon/android-icon-192x192.png">
    <link rel="icon" type="image/png" sizes="32x32" href="/{{ directory }}/src/images/favicon/favicon-32x32.png">
    <link rel="icon" type="image/png" sizes="96x96" href="/{{ directory }}/src/images/favicon/favicon-96x96.png">
    <link rel="icon" type="image/png" sizes="16x16" href="/{{ directory }}/src/images/favicon/favicon-16x16.png">
    <link rel="manifest" href="/{{ directory }}/manifest.json" crossorigin="use-credentials">
    <meta name="msapplication-TileColor" content="#ffffff">
    <meta name="msapplication-TileImage" content="/{{ directory }}/src/images/favicon/ms-icon-144x144.png">
    <meta name="theme-color" content="#de1738"/>
  </head>
  <body{{ attributes.addClass(body_classes) }}>
    {# Keyboard navigation/accessibility link to main content section in page.html.twig. #}
    <a href="#main-content" class="visually-hidden focusable skip-link">
      {{ 'Skip to main content' | t }}
    </a>
    {{ page_top }}
    {{ page }}
    {{ page_bottom }}
    <js-bottom-placeholder token="{{ placeholder_token }}">
  </body>
</html>
